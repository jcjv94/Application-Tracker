<%- include('../partials/header') %>

<h1>Applications</h1>

<table class="highlight z-depth-5">
    <thead>
        <div>
            <th class="center-align">Job Title</th>
            <th class="center-align">Employer</th>
            <th class="center-align">Location</th>
            <th class="center-align">Date Applied</th>
            <th class="center-align">Salary Range</th>
            <th class="center-align">Notes</th>
        </div>
    </thead>
    <tbody>
        
        <% for (app of applications) { %>
            <tr>
                <td class="center-align"><%= app.jobTitle %></td>
                <td class="center-align"><%=app.employer%></td>
                <td class="center-align"><%=app.location%></td>
                <td class="center-align"><%=app.dateApplied%></td>
                <td class="center-align"><%=app.salaryRange%></td>
                <td class="center-align"><%=app.notes%></td>
            </tr>
            <% } %>

        </tbody>
    </table>
<%- include('../partials/footer') %>
